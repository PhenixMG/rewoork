{
  "name": "rewoork",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch --env-file=.env src/bot.js",
    "start": "node --env-file=.env src/bot.js",
    "db:init": "prisma init --datasource-provider mysql",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "lint": "eslint .",
    "check": "npm run lint",
    "compose:up": "docker compose up -d --build",
    "compose:down": "docker compose down",
    "compose:logs": "docker compose logs -f bot",
    "compose:ps": "docker compose ps",
    "db:push:docker": "docker compose exec bot npx prisma db push",
    "db:migrate:docker": "docker compose exec bot npx prisma migrate deploy",
    "db:studio:docker": "docker compose exec -it bot npx prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "discord.js": "^14.21.0",
    "luxon": "^3.7.1",
    "mysql2": "^3.14.3",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "prisma": "^6.13.0"
  }
}
